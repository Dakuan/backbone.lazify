{
  "name": "backbone.lazify",
  "version": "0.0.4",
  "description": "Call any method lazily",
  "main": "dist/backbone.lazify.js",
  "scripts": {
    "test": "mocha --compilers js:babel/register",
    "build": "npm run build:src && npm run build:min",
    "prepublish": "npm run build",
    "build:src": "babel src/backbone.lazify.es6 | umd backbone.lazify > ./dist/backbone.lazify.js",
    "build:min": "babel src/backbone.lazify.es6 | umd backbone.lazify | uglifyjs > ./dist/backbone.lazify.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dakuan/backbone.lazify.git"
  },
  "author": {
    "name": "Dom Barker",
    "email": "dom@terriertech.io",
    "url": "http://www.terriertech.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dakuan/backbone.lazify/issues"
  },
  "homepage": "https://github.com/Dakuan/backbone.lazify",
  "dependencies": {
    "backbone": "^1.2.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "chai": "^3.2.0",
    "chai-spies": "^0.7.0",
    "mocha": "^2.2.5",
    "uglify": "^0.1.5",
    "uglify-js": "^2.4.24",
    "umd": "^3.0.1"
  }
}
